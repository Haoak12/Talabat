<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
            fx:controller="Talabat.Controllers.AdminController">
    <children>
        <Label layoutX="206.0" layoutY="27.0" text="Your Restaurants">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <ChoiceBox fx:id="cb" layoutX="198.0" layoutY="120.0" prefWidth="150.0"/>
        <Label layoutX="135.0" layoutY="66.0" text="Choose Resturant you want to Edit">
            <font>
                <Font size="19.0"/>
            </font>
        </Label>
        <Button layoutX="35.0" layoutY="340.0" mnemonicParsing="false" onAction="#switchtoadmin" prefHeight="26.0"
                prefWidth="74.0" text="back"/>
        <Button layoutX="443.0" layoutY="146.0" mnemonicParsing="false" onAction="#DeleteRestaurant"
                text="Delete Restaurant"/>
        <Button layoutX="443.0" layoutY="200.0" mnemonicParsing="false" text="Edit Restaurant menu"/>
    </children>
</AnchorPane>
